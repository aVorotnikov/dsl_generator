    'TERMINALS'; 'KEYS'; 'NONTERMINALS'; 'AXIOM'; 'RULES'; 'ERRORS';
    '.'; ':'; '::='; ';'; '('; ')'; '|'; '['; ']'; '{'; '}'; '#'.

NONTERMINALS:
    GRAMMAR;
    TERMINALS_BLOCK;
    KEYS_BLOCK;
    NONTERMINALS_BLOCK;
    AXIOM_BLOCK;
    ERROR_BLOCK;
    RULES_BLOCK;
    RULE;
    RHS;
    SEQUENCE;
    BRACKETS;
    OPTIONAL;
    TSEITIN_ITERATION.

AXIOM: GRAMMAR.
